{
  "_version": "1.60.0",
  "sap.app": {
    "id": "podetails",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.3",
      "toolsId": "19ddbcc4-874b-4f80-a054-6601f7357bd5"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZPO_UI5_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "podetails.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "podetails.view",
        "async": true,
        "viewPath": "podetails.view"
      },
      "routes": [
        {
          "name": "RoutePurchaseOrderHeader",
          "pattern": ":?query:",
          "target": [
            "TargetPurchaseOrderHeader"
          ]
        },
        {
          "name": "RoutePurchaseOrderItem",
          "pattern": "RoutePurchaseOrderItem/{PONumber}",
          "target": "TargetRoutePurchaseOrderItem"
        }
      ],
      "targets": {
        "TargetPurchaseOrderHeader": {
          "id": "POHeaderPage",
          "name": "PurchaseOrderHeader"
        },
        "TargetRoutePurchaseOrderItem": {
          "id": "POItemPage",
          "name": "PurchaseOrderItem"
        }
      }
    },
    "rootView": {
      "viewName": "podetails.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}

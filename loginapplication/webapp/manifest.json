{
  "_version": "1.65.0",
  "sap.app": {
    "id": "loginapplication",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.2",
      "toolsId": "a6be57e6-a9ff-45d4-906c-1c353a584301"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.138.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "loginapplication.i18n.i18n"
        }
      },
      "Datas" : {
        "type" : "sap.ui.model.json.JSONModel",
        "uri" : "model/Data.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "loginapplication.view",
        "async": true,
        "viewPath": "loginapplication.view"
      },
      "routes": [
        {
          "name": "RouteLoginPage",
          "pattern": ":?query:",
          "target": [
            "TargetLoginPage"
          ]
        },
        {
          "name": "RouteMainPage",
          "pattern": "RouteMainPage",
          "target": "TargetMainPage"
        },
        {
          "name": "RoutePurchaseOrderCreatePage",
          "pattern": "RoutePurchaseOrderCreatePage",
          "target": "TargetPurchaseOrderCreatePage"
        }
      ],
      "targets": {
        "TargetLoginPage": {
          "id": "LoginPage",
          "name": "LoginPage"
        },
        "TargetMainPage": {
          "id": "MainPage",
          "name": "MainPage"
        },
        "TargetPurchaseOrderCreatePage": {
          "id": "PurchaseOrderCreate",
          "name": "PurchaseOrderCreate"
        }
      }
    },
    "rootView": {
      "viewName": "loginapplication.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
